<%- include('../header') -%>
<%- include('../nav') -%>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <div class="card border-success mb-3">
        <div class="card-header">Search</div>
        <div class="card-body">
          <form>
            <div class="form-row">
              <div class="form-group col-md-12">
                <label for="name">매입처 명</label>
                <div class="input-group mb-3">
                  <input id="name" type="text" class="form-control" placeholder="검색하실려는 매입처 명을 입력하세요." aria-label="검색하실려는 매입처 명을 입력하세요." aria-describedby="search">
                  <div class="input-group-append">
                    <button class="btn btn-outline-success" type="button" id="search">검색</button>
                  </div>
                </div>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
    <div class="col-md-12" style="margin-bottom: 1em;">
      <div class="row">
        <div class="col-md-6 text-left">
          <a class="btn btn-outline-warning" href="/corporation/add">매입처 추가</a>
        </div>
        <div class="col-md-6 text-right">
          <p class="read align-bottom">총 <span id="qty">0</span>건의 자료가 있습니다.</p>
        </div>
      </div>
    </div>
    <div class="col-md-12">
      <table class="table">
        <thead>
          <tr class="table-info">
            <th scope="col" class="text-center">매입처 아이디</th>
            <th scope="col" class="text-center">매입처 명</th>
            <th scope="col" class="text-center">전화번호</th>
            <th scope="col" class="text-center">&nbsp;</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
    <div class="col-md-12 text-xs-center">
      <ul class="pagination justify-content-center" id="pagenation">
      </ul>
    </div>
  </div>
</div>
<%- include('../footer') -%>