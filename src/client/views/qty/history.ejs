<%- include('../header') -%>
<%- include('../nav') -%>
<div class="container-fluid">
  <div class="row">
    <div class="col-md-12">
      <div class="card border-success mb-3">
        <div class="card-header">매입/매출 검색</div>
        <div class="card-body">
          <form>
            <div class="form-row">
              <div class="form-group col-md-3">
                <label for="inputState">구분</label>
                <select class="form-control" id="selType">
                  <option value="">모두</option>
                  <option value=">">매입</option>
                  <option value="<">매출</option>
                </select>
              </div>
              <div class="form-group col-md-3">
                <label for="inputState">매입처</label>
                <select class="form-control" id="selCo">
                  <option value="">모두</option>
                </select>
              </div>
              <div class="form-group col-md-3">
                <label for="name">기간</label>
                <input type="text" class="form-control" id="sdate">
              </div>
            </div>
            <div class="form-row">
              <div class="form-group col-md-3">
                <label for="inputState">카테고리1</label>
                <select class="form-control" id="sel1">
                  <option value="">모두</option>
                </select>
              </div>
              <div class="form-group col-md-3">
                <label for="inputState">카테고리2</label>
                <select class="form-control" id="sel2">
                  <option value="">모두</option>
                </select>
              </div>
              <div class="form-group col-md-3">
                <label for="inputState">카테고리3</label>
                <select class="form-control" id="sel3">
                  <option value="">모두</option>
                </select>
              </div>
              <div class="form-group col-md-3">
                <label for="name">제품명</label>
                <input type="text" class="form-control" id="name">
              </div>
            </div>
            <button class="btn btn-outline-success" type="button" id="search">검색</button>
          </form>
        </div>
      </div>
    </div>
    <div class="col-md-12 text-right" style="margin-bottom: 1em;">
      <p class="read align-bottom">총 <span id="qty">0</span>건의 자료가 있습니다.</p>
    </div>
    <div class="col-md-12">
      <table class="table">
        <thead>
          <tr class="table-info">
            <th scope="col" class="text-center">ID</th>
            <th scope="col" class="text-center">매입처</th>
            <th scope="col" class="text-center">카테고리</th>
            <th scope="col" class="text-center">제품명</th>
            <th scope="col" class="text-center">비고</th>
            <th scope="col" class="text-center">수량</th>
            <th scope="col" class="text-center">가격</th>
            <th scope="col" class="text-center">날짜</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
    <div class="col-md-12 text-xs-center">
      <ul class="pagination justify-content-center" id="pagenation">
      </ul>
    </div>
  </div>
</div>
<%- include('../footer') -%>